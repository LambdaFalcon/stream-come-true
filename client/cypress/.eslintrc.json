{
  "plugins": ["cypress", "chai-friendly"],
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2
  },
  "env": {
    "red-over-time-api": "http://localhost:9000/api/v1/reddit/items_over_time]",
    "tweet-over-time-api": "http://localhost:9000/api/v1/twitter/items_over_time",
    "red-popular-keywords-api": "http://localhost:9000/api/v1/reddit/popular_keywords",
    "tweet-popular-keywords-api": "http://localhost:9000/api/v1/twitter/popular_keywords",
    "red-users-over-time-api": "http://localhost:9000/api/v1/reddit/users_over_time",
    "tweet-users-over-time-api": "http://localhost:9000/api/v1/twitter/users_over_time",
    "red-pop-users-api": "http://localhost:9000/api/v1/reddit/popular_users",
    "tweet-pop-users-api": "http://localhost:9000/api/v1/twitter/popular_users",
    "red-sent-analysis-api": "http://localhost:9000/api/v1/reddit/sentiment",
    "tweet-sent-analysis-api": "http://localhost:9000/api/v1/twitter/sentiment",
    "red_data": "http://localhost:9000/api/v1/reddit/",
    "tweet_data": "http://localhost:9000/api/v1/twitter/"
  },
  "extends": ["plugin:cypress/recommended"]
}
